# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal with noncollinear spins
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Processors : 2

Input      : 07-noncollinear.01-U5-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  Precision: 1e-8
  match ;  SCF convergence  ; GREPCOUNT(static/info, 'SCF converged') ; 1.0
  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 2.0

  Precision: 2.6e-3
  match ;   Total energy         ; GREPFIELD(static/info, 'Total       =', 3) ; -246.660892745
  Precision: 1e-8
  match ;   Ion-ion energy       ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -177.00987193
  Precision: 1.8e-3
  match ;   Eigenvalues sum      ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -31.85600557
  Precision: 7.7e-4
  match ;   Hartree energy       ; GREPFIELD(static/info, 'Hartree     =', 3) ; 48.182984535
  Precision: 1.2e-4
  match ;   Exchange energy      ; GREPFIELD(static/info, 'Exchange    =', 3) ; -26.555601510000002
  Precision: 6.8e-06
  match ;   Correlation energy   ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.784742805
  Precision: 1.2e-3
  match ;   Kinetic energy       ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 125.150107255
  Precision: 1.9e-3
  match ;   External energy      ; GREPFIELD(static/info, 'External    =', 3) ; -213.95688684499999
  Precision: 8.4e-06
  match ;   Hubbard energy       ; GREPFIELD(static/info, 'Hubbard     =', 3) ; 0.311138435

  Precision: 1e-8
  match ;   Total Magnetic Moment x   ; GREPFIELD(static/info, 'mx = ', 3) ; 0.0
  match ;   Total Magnetic Moment y   ; GREPFIELD(static/info, 'my = ', 3) ; 0.0
  match ;   Total Magnetic Moment z   ; GREPFIELD(static/info, 'mz = ', 3) ; 0.0

  Precision: 4.5e-4
  match ;   Local Magnetic Moment (Ni1)   ; GREPFIELD(static/info, '1        Ni', 3) ; 2.5479575
  match ;   Local Magnetic Moment (Ni2)   ; GREPFIELD(static/info, '2        Ni', 3) ; -2.5479575
  Precision: 1e-8
  match ;   Local Magnetic Moment (O1)   ; GREPFIELD(static/info, '3         O', 3) ; 0.0
  match ;   Local Magnetic Moment (O2)   ; GREPFIELD(static/info, '4         O', 3) ; 0.0

  Precision: 1e-15
  match ;   k-point 1 (x)   ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  match ;   k-point 1 (y)   ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  match ;   k-point 1 (z)   ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0

  Precision: 7.3e-05
  match ;   Eigenvalue  1   ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.945946
  match ;   Eigenvalue  8   ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -2.8461204999999996
  match ;   Eigenvalue 16   ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.46941900000000003
  match ;   Eigenvalue 17   ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.17454150000000002

  Precision: 1.4e-4
  match ;   Occupation Ni2 up-down 3d4   ; LINEFIELD(static/occ_matrices, -2, 7) ; -0.035771315
  match ;   Occupation Ni2 up-down 3d5   ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.390588
endif

