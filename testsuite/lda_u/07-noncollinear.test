# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal with noncollinear spins
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Processors : 2

Input      : 07-noncollinear.01-U5-gs.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 2

match ;  SCF convergence  ; GREPCOUNT(static/info, 'SCF converged') ; 1.0
match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4.0
match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 2.0

Precision: 2.8e-05
match ;   Total energy         ; GREPFIELD(static/info, 'Total       =', 3) ; -246.663112
Precision: 1e-8
match ;   Ion-ion energy       ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -177.00987193
Precision: 4.4e-07
match ;   Eigenvalues sum      ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -31.857183929999998
Precision: 5.6e-05
match ;   Hartree energy       ; GREPFIELD(static/info, 'Hartree     =', 3) ; 48.183771394999994
Precision: 5.7e-06
match ;   Exchange energy      ; GREPFIELD(static/info, 'Exchange    =', 3) ; -26.555778275
Precision: 6.1e-07
match ;   Correlation energy   ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.78473787
Precision: 5.3e-05
match ;   Kinetic energy       ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 125.15100539
Precision: 1.1e-4
match ;   External energy      ; GREPFIELD(static/info, 'External    =', 3) ; -213.95842095
Precision: 6.1e-07
match ;   Hubbard energy       ; GREPFIELD(static/info, 'Hubbard     =', 3) ; 0.31115511500000004

Precision: 1e-15
match ;   Total Magnetic Moment x   ; GREPFIELD(static/info, 'mx = ', 3) ; 0.0
match ;   Total Magnetic Moment y   ; GREPFIELD(static/info, 'my = ', 3) ; 0.0
match ;   Total Magnetic Moment z   ; GREPFIELD(static/info, 'mz = ', 3) ; 0.0

Precision: 2.5e-05
match ;   Local Magnetic Moment (Ni1)   ; GREPFIELD(static/info, '1        Ni', 3) ;  2.548203
match ;   Local Magnetic Moment (Ni2)   ; GREPFIELD(static/info, '2        Ni', 3) ; -2.548203

Precision: 1e-15
match ;   Local Magnetic Moment (O1)   ; GREPFIELD(static/info, '3         O', 3) ; 0.0
match ;   Local Magnetic Moment (O2)   ; GREPFIELD(static/info, '4         O', 3) ; 0.0

match ;   k-point 1 (x)   ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
match ;   k-point 1 (y)   ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
match ;   k-point 1 (z)   ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0

Precision: 2.3e-06
match ;   Eigenvalue  1   ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.946045
match ;   Eigenvalue  8   ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -2.8461695000000002
match ;   Eigenvalue 16   ; GREPFIELD(static/info, '#k =   1', 3, 16) ; -0.4694245
match ;   Eigenvalue 17   ; GREPFIELD(static/info, '#k =   1', 3, 17) ; -0.1745445

Precision: 2.5e-05
match ;   Occupation Ni2 up-down 3d4   ; LINEFIELD(static/occ_matrices, -2, 7) ; -0.036018775
match ;   Occupation Ni2 up-down 3d5   ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.390451905


Input      : 07-noncollinear.02-acbn0.inp

