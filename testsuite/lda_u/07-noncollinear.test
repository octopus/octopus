  Precision: 2.75e-08
  match ;   Hubbard energy       ; GREPFIELD(static/info, 'Hubbard     =', 3) ; 0.17158253499999998

  Precision: 1.00e-15
  match ;   Total Magnetic Moment x   ; GREPFIELD(static/info, 'mx = ', 3) ; 0.0
  match ;   Total Magnetic Moment y   ; GREPFIELD(static/info, 'my = ', 3) ; 0.0
  match ;   Total Magnetic Moment z   ; GREPFIELD(static/info, 'mz = ', 3) ; 0.0
  Precision: 1.83e-15
  match ;   Local Magnetic Moment (Ni1)   ; GREPFIELD(static/info, '1        Ni', 3) ; 1.825908
  match ;   Local Magnetic Moment (Ni2)   ; GREPFIELD(static/info, '2        Ni', 3) ; -1.825908
  Precision: 1.00e-15
  match ;   Local Magnetic Moment (O1)   ; GREPFIELD(static/info, '3         O', 3) ; 0.0
  match ;   Local Magnetic Moment (O2)   ; GREPFIELD(static/info, '4         O', 3) ; 0.0

  Precision: 1.00e-15
  match ;   k-point 1 (x)   ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  match ;   k-point 1 (y)   ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  match ;   k-point 1 (z)   ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0

  Precision: 5.19e-16
  match ;   Eigenvalue  1   ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.518505
  Precision: 7.43e-17
  match ;   Eigenvalue  8   ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -0.074302
  Precision: 5.50e-07
  match ;   Eigenvalue 16   ; GREPFIELD(static/info, '#k =   1', 3, 16) ; 0.0322735
  Precision: 4.31e-17
  match ;   Eigenvalue 17   ; GREPFIELD(static/info, '#k =   1', 3, 17) ; 0.043058
  Precision: 4.40e-08
  match ;   Occupation Ni2 up-down 3d4   ; LINEFIELD(static/occ_matrices, -2, 7) ; -0.01876749
  Precision: 9.90e-08
  match ;   Occupation Ni2 up-down 3d5   ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.44083197  

endif

