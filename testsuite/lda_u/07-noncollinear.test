# -*- coding: utf-8 mode: shell-script -*-

Test       : GGA+U for (AF) bulk NiO crystal with noncollinear spins
Program    : octopus
TestGroups : long-run, periodic_systems, lda_u
Enabled    : Yes

Processors : 2

Input      : 07-noncollinear.01-U5-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  Precision: 1e-8
  match ;  SCF convergence  ; GREPCOUNT(static/info, 'SCF converged') ; 1.0
  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 2.0

  Precision: 2.04e-07
  match ;   Total energy         ; GREPFIELD(static/info, 'Total       =', 3) ; -90.195310385
  Precision: 8.54e-14
  match ;   Ion-ion energy       ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -85.44424755
  Precision: 3.30e-08
  match ;   Eigenvalues sum      ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; 1.98886071
  Precision: 1.92e-07
  match ;   Hartree energy       ; GREPFIELD(static/info, 'Hartree     =', 3) ; 13.264719894999999
  Precision: 3.30e-08
  match ;   Exchange energy      ; GREPFIELD(static/info, 'Exchange    =', 3) ; -15.819583770000001
  Precision: 1.79e-15
  match ;   Correlation energy   ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.7891743
  Precision: 4.40e-07
  match ;   Kinetic energy       ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 80.69671912999999
  Precision: 5.66e-07
  match ;   External energy      ; GREPFIELD(static/info, 'External    =', 3) ; -81.376037825
  Precision: 1.10e-08
  match ;   Hubbard energy       ; GREPFIELD(static/info, 'Hubbard     =', 3) ; 0.27230894

  Precision: 3.64e-15
  match ;   Total Magnetic Moment x   ; GREPFIELD(static/info, 'mx = ', 3) ; 0.0
  match ;   Total Magnetic Moment y   ; GREPFIELD(static/info, 'my = ', 3) ; 0.0
  match ;   Total Magnetic Moment z   ; GREPFIELD(static/info, 'mz = ', 3) ; 0.0
  match ;   Local Magnetic Moment (Ni1)   ; GREPFIELD(static/info, '1        Ni', 3) ; 3.644446
  match ;   Local Magnetic Moment (Ni2)   ; GREPFIELD(static/info, '2        Ni', 3) ; -3.644446
  match ;   Local Magnetic Moment (O1)   ; GREPFIELD(static/info, '3         O', 3) ; 0.0
  match ;   Local Magnetic Moment (O2)   ; GREPFIELD(static/info, '4         O', 3) ; 0.0
  
  Precision: 1.00e-15
  match ;   k-point 1 (x)   ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  match ;   k-point 1 (y)   ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  match ;   k-point 1 (z)   ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0
 
  Precision: 3.84e-16
  match ;   Eigenvalue  1   ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.383971
  match ;   Eigenvalue  8   ; GREPFIELD(static/info, '#k =   1', 3, 8) ; -0.013981
  match ;   Eigenvalue 16   ; GREPFIELD(static/info, '#k =   1', 3, 16) ; 0.086589
  match ;   Eigenvalue 17   ; GREPFIELD(static/info, '#k =   1', 3, 17) ; 0.109796

  Precision: 4.95e-08
  match ;   Occupation Ni2 up-down 3d4   ; LINEFIELD(static/occ_matrices, -2, 7) ; -0.06310971500000001
  match ;   Occupation Ni2 up-down 3d5   ; LINEFIELD(static/occ_matrices, -1, 9) ; -0.395657735

endif

