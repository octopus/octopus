# -*- coding: utf-8 mode: shell-script -*-

Test       : ACBN0 functional for isolated systems
Program    : octopus
TestGroups : short-run, finite_systems_3d, lda_u
Enabled    : Yes

Input      : 04-ACBN0_isolated.01-H.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ;  -0.47032448
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ;  -0.53642246
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;   0.30783264
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ;  -0.26369204
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ;  -0.02221794
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;   0.47490473
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ;  -0.97488130
match ; Hubbard energy     ; GREPFIELD(static/info, 'Hubbard     =', 3) ;   0.00771558

match ; Eigenvalue   [1up] ; GREPFIELD(static/info, '1   up', 3) ; -0.536422
match ; Occupation   [1up] ; GREPFIELD(static/info, '1   up', 4) ; 1.000000
match ; Eigenvalue [1down] ; GREPFIELD(static/info, '1   dn', 3) ; 0.101149
match ; Eigenvalue   [2up] ; GREPFIELD(static/info, '2   up', 3) ; 0.102119
match ; Eigenvalue   [2up] ; GREPFIELD(static/info, '2   dn', 3) ; 0.140110


match ; Occupation 1s up ; LINEFIELD(static/occ_matrices, -3, 1) ; 0.97344171

match ; U  ; LINEFIELD(static/effectiveU, -1, 4) ; 0.596803
