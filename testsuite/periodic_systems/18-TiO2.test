# -*- coding: utf-8 mode: shell-script -*-

Test       : TiO2 and matrix elements
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Input      : 18-TiO2.01-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 6.0
  match ;  Space group         ; GREPFIELD(out, 'Space group', 4) ; 136.0
  match ;  No. of symmetries   ; GREPFIELD(out, 'symmetries that can be used', 5) ; 8.0

  Precision: 3.42e-05
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -183.94935119500002
  Precision: 1.19e-13
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -118.71359251
  Precision: 1.65e-05
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -27.91587269
  Precision: 1.94e-05
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 41.70020216
  Precision: 4.09e-06
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -31.30061154
  Precision: 1.48e-07
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.2669297349999997
  Precision: 2.22e-05
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 86.374677905
  Precision: 3.81e-05
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -159.743108145

  Precision: 1.00e-15
  match ;  k-point 1 (x)  ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  match ;  k-point 1 (y)  ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  match ;  k-point 1 (z)  ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0

  Precision: 2.75e-06
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.0018355
  Precision: 5.50e-07
  match ;  Eigenvalue  2  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -1.9927285000000001
  Precision: 2.20e-06
  match ;  Eigenvalue  4  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -1.212923
  Precision: 2.75e-06
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; -1.2069925000000001

  Precision: 5.06e-17
  match ;  Direct gap  ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.0506
  Precision: 1.99e-17
  match ;  Indirect gap  ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.0199

  Precision: 1.65e-07
  match ;  Two-body (vvvv) Re  ; LINEFIELD(static/output_me_two_body, 2, 9) ; 0.05450985
  Precision: 2.90e-19
  match ;  Two-body (vvvv) Im  ; LINEFIELD(static/output_me_two_body, 2, 10) ; -1.309375e-19
  Precision: 6.43e-05
  match ;  Two-body (cccc) Re  ; LINEFIELD(static/output_me_two_body, 1887, 9) ; 0.1921755
  Precision: 4.89e-19
  match ;  Two-body (cccc) Im  ; LINEFIELD(static/output_me_two_body, 1887, 10) ; 2.222705e-19
  Precision: 2.52e-15
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 9) ; -2.4402999999999994e-16
  Precision: 4.83e-20
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 10) ; 6.88815e-21
endif
