# -*- coding: utf-8 mode: shell-script -*-

Test       : TiO2 and matrix elements
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Input      : 18-TiO2.01-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

  match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
  match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 6
  match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 136
  match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  8

  Precision: 4.1e-05
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -183.94936382999998
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -118.71359251
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -27.915882645
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 41.700197705
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -31.30061765
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.266929485
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 86.37468856000001
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -159.743108965

  Precision: 1e-8
  match ;  k-point 1 (x)  ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  match ;  k-point 1 (y)  ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  match ;  k-point 1 (z)  ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0
  Precision: 4.5e-06
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.0018339999999997
  match ;  Eigenvalue  2  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -1.9927285000000001
  match ;  Eigenvalue  4  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -1.2129215
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; -1.2069915

  Precision: 1.0e-06
  match ;  Two-body (vvvv) Re  ; LINEFIELD(static/output_me_two_body, 2, 9) ; 0.0545094
  Precision: 1.0e-18
  match ;  Two-body (vvvv) Im  ; LINEFIELD(static/output_me_two_body, 2, 10) ; 1.503465e-19

  Precision: 1e-8
  match ;  Direct gap  ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.0506
  match ;  Indirect gap  ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.0199

  Precision: 1.4e-4
  match ;  Two-body (cccc) Re  ; LINEFIELD(static/output_me_two_body, 1887, 9) ; 0.19252350000000001
  Precision: 5.0e-14
  match ;  Two-body (cccc) Im  ; LINEFIELD(static/output_me_two_body, 1887, 10) ; 4.791405e-19
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 9) ; -6.465455e-16
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 10) ; 2.8857500000000023e-21
endif