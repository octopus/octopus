# -*- coding: utf-8 mode: shell-script -*-

Test       : TiO2 and matrix elements
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Input      : 18-TiO2.01-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 6.0
  match ;  Space group         ; GREPFIELD(out, 'Space group', 4) ; 136.0
  match ;  No. of symmetries   ; GREPFIELD(out, 'symmetries that can be used', 5) ; 8.0

  Precision: 3.13e-05
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -183.94936274999998
  Precision: 1.19e-13
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -118.71359251
  Precision: 1.31e-05
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -27.915883745000002
  Precision: 1.85e-05
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 41.700200425000006
  Precision: 2.10e-06
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -31.30060991
  Precision: 1.37e-07
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.266929855
  Precision: 1.57e-05
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 86.37466905
  Precision: 3.35e-05
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -159.74310094499998
  Precision: 1.00e-15
  match ;  k-point 1 (x)  ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  Precision: 1.00e-15
  match ;  k-point 1 (y)  ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  Precision: 1.00e-15
  match ;  k-point 1 (z)  ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0
  Precision: 2.20e-06
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.0018380000000002
  Precision: 1.10e-06
  match ;  Eigenvalue  2  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -1.992728
  Precision: 2.20e-06
  match ;  Eigenvalue  4  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -1.212925
  Precision: 2.20e-06
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; -1.206995
  Precision: 1.65e-07
  match ;  Two-body (vvvv) Re  ; LINEFIELD(static/output_me_two_body, 2, 9) ; 0.05450995
  Precision: 3.24e-19
  match ;  Two-body (vvvv) Im  ; LINEFIELD(static/output_me_two_body, 2, 10) ; 1.009975e-19
  Precision: 5.06e-17
  match ;  Direct gap  ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.0506
  Precision: 1.99e-17
  match ;  Indirect gap  ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.0199
  Precision: 7.42e-05
  match ;  Two-body (cccc) Re  ; LINEFIELD(static/output_me_two_body, 1887, 9) ; 0.1921775
  Precision: 1.70e-18
  match ;  Two-body (cccc) Im  ; LINEFIELD(static/output_me_two_body, 1887, 10) ; -6.776174999999999e-19
  Precision: 3.00e-15
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 9) ; 4.5203e-16
  Precision: 5.91e-20
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 10) ; -1.2966200000000003e-20
endif
