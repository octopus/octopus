# -*- coding: utf-8 mode: shell-script -*-

Test       : TiO2 and matrix elements
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Input      : 18-TiO2.01-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 6.0
  match ;  Space group         ; GREPFIELD(out, 'Space group', 4) ; 136.0
  match ;  No. of symmetries   ; GREPFIELD(out, 'symmetries that can be used', 5) ; 8.0

  Precision: 3.08e-05
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -183.94938530500002
  Precision: 1.19e-13
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -118.71359251
  Precision: 1.42e-05
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -27.915893115000003
  Precision: 1.93e-05
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 41.70021131
  Precision: 2.01e-06
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -31.300609989999998
  Precision: 1.98e-07
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.26692991
  Precision: 1.30e-05
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 86.37467710499999
  Precision: 3.14e-05
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -159.7431205
  Precision: 1.00e-15
  match ;  k-point 1 (x)  ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  Precision: 1.00e-15
  match ;  k-point 1 (y)  ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  Precision: 1.00e-15
  match ;  k-point 1 (z)  ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0
  Precision: 2.75e-06
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.0018385
  Precision: 1.10e-06
  match ;  Eigenvalue  2  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -1.992728
  Precision: 2.75e-06
  match ;  Eigenvalue  4  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -1.2129254999999999
  Precision: 2.75e-06
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; -1.2069955
  Precision: 3.30e-07
  match ;  Two-body (vvvv) Re  ; LINEFIELD(static/output_me_two_body, 2, 9) ; 0.0545098
  Precision: 4.61e-19
  match ;  Two-body (vvvv) Im  ; LINEFIELD(static/output_me_two_body, 2, 10) ; -2.36485e-20
  Precision: 5.06e-17
  match ;  Direct gap  ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.0506
  Precision: 1.99e-17
  match ;  Indirect gap  ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.0199
  Precision: 4.34e-05
  match ;  Two-body (cccc) Re  ; LINEFIELD(static/output_me_two_body, 1887, 9) ; 0.1921695
  Precision: 1.61e-18
  match ;  Two-body (cccc) Im  ; LINEFIELD(static/output_me_two_body, 1887, 10) ; 3.6978500000000005e-19
  Precision: 2.36e-15
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 9) ; -1.536649999999999e-16
  Precision: 4.88e-20
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 10) ; -2.231235e-20
endif
