# -*- coding: utf-8 mode: shell-script -*-

Test       : TiO2 and matrix elements
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Input      : 18-TiO2.01-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 6.0
  match ;  Space group         ; GREPFIELD(out, 'Space group', 4) ; 136.0
  match ;  No. of symmetries   ; GREPFIELD(out, 'symmetries that can be used', 5) ; 8.0

  Precision: 1.94e-05
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -183.94935152
  Precision: 1.19e-13
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -118.71359251
  Precision: 9.45e-06
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -27.915878965
  Precision: 1.27e-05
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 41.700198615
  Precision: 6.31e-06
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -31.300613075
  Precision: 3.46e-07
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.266929625
  Precision: 1.41e-05
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 86.374681255
  Precision: 1.90e-05
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -159.743110635

  Precision: 5.50e-06
  match ;  k-point 1 (x)  ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  match ;  k-point 1 (y)  ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  match ;  k-point 1 (z)  ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.001835
  match ;  Eigenvalue  2  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -1.992728
  match ;  Eigenvalue  4  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -1.212922
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; -1.206992

  Precision: 4.95e-07
  match ;  Two-body (vvvv) Re  ; LINEFIELD(static/output_me_two_body, 2, 9) ; 0.054510050000000004
  Precision: 7.63e-20
  match ;  Two-body (vvvv) Im  ; LINEFIELD(static/output_me_two_body, 2, 10) ; 6.9399e-20

  Precision: 5.06e-17
  match ;  Direct gap  ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.0506
  match ;  Indirect gap  ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.0199

  Precision: 5.99e-05
  match ;  Two-body (cccc) Re  ; LINEFIELD(static/output_me_two_body, 1887, 9) ; 0.19235750000000001
  Precision: 1.79e-18
  match ;  Two-body (cccc) Im  ; LINEFIELD(static/output_me_two_body, 1887, 10) ; 8.386059999999999e-19
  Precision: 2.15e-15
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 9) ; -4.1839999999999907e-17
  Precision: 3.34e-20
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 10) ; 3.03555e-20
endif
