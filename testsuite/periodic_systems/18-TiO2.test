# -*- coding: utf-8 mode: shell-script -*-

Test       : TiO2 and matrix elements
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Input      : 18-TiO2.01-gs.inp

if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

  match ;  Total k-points    ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8.0
  match ;  Reduced k-points  ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 6.0
  match ;  Space group         ; GREPFIELD(out, 'Space group', 4) ; 136.0
  match ;  No. of symmetries   ; GREPFIELD(out, 'symmetries that can be used', 5) ; 8.0

  Precision: 3.05e-05
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -183.94937647
  Precision: 1.19e-13
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -118.71359251
  Precision: 1.66e-05
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -27.91589105
  Precision: 1.47e-05
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 41.700205194999995
  Precision: 7.83e-06
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -31.300610525
  Precision: 4.62e-07
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -2.26692982
  Precision: 8.35e-06
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 86.37467602999999
  Precision: 1.53e-05
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -159.74310934
  Precision: 1.00e-15
  match ;  k-point 1 (x)  ; GREPFIELD(static/info, '#k =   1', 7) ; 0.0
  Precision: 1.00e-15
  match ;  k-point 1 (y)  ; GREPFIELD(static/info, '#k =   1', 8) ; 0.0
  Precision: 1.00e-15
  match ;  k-point 1 (z)  ; GREPFIELD(static/info, '#k =   1', 9) ; 0.0
  Precision: 7.70e-06
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -2.001837
  Precision: 3.85e-06
  match ;  Eigenvalue  2  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -1.9927315
  Precision: 7.15e-06
  match ;  Eigenvalue  4  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -1.2129235
  Precision: 6.60e-06
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; -1.206994
  Precision: 1.21e-06
  match ;  Two-body (vvvv) Re  ; LINEFIELD(static/output_me_two_body, 2, 9) ; 0.054509100000000005
  Precision: 6.17e-19
  match ;  Two-body (vvvv) Im  ; LINEFIELD(static/output_me_two_body, 2, 10) ; 1.25519e-19
  Precision: 5.06e-17
  match ;  Direct gap  ; GREPFIELD(static/info, 'Direct gap', 7, 0) ; 0.0506
  Precision: 1.99e-17
  match ;  Indirect gap  ; GREPFIELD(static/info, 'Indirect gap', 10, 0) ; 0.0199
  Precision: 1.55e-04
  match ;  Two-body (cccc) Re  ; LINEFIELD(static/output_me_two_body, 1887, 9) ; 0.192549
  Precision: 9.73e-19
  match ;  Two-body (cccc) Im  ; LINEFIELD(static/output_me_two_body, 1887, 10) ; -8.84645e-19
  Precision: 2.38e-15
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 9) ; 2.1280000000000022e-17
  Precision: 5.14e-20
  match ;  Two-body (vvcc) Re  ; LINEFIELD(static/output_me_two_body, 14, 10) ; 1.808635e-20
endif
