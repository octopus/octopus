match ; Energy           [step 20] ; LINE(td.general/energy, -11, 30) ; -0.537556
match ; Energy           [step 35] ; LINE(td.general/energy,  -6, 30) ; -0.537556
match ; Energy           [step 30] ; LINE(td.general/energy,  -1, 30) ; -0.537556

