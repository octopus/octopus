match ; Vector potential [step  1] ; LINE(td.general/gauge_field, -21, 30) ; 1.000000000000e-01
match ; Vector potential [step  5] ; LINE(td.general/gauge_field, -16, 30) ; 9.995786620968e-02
match ; Vector potential [step 10] ; LINE(td.general/gauge_field, -11, 30) ; 9.983204192579e-02
match ; Vector potential [step 15] ; LINE(td.general/gauge_field,  -6, 30) ; 9.962286381526e-02
