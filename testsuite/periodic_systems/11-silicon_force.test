# -*- coding: utf-8 mode: shell-script -*-

Test       : Silicon force
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Precision : 1e-8

Input      : 11-silicon_force.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 8
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 4
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 1
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  1

match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -32.05812329
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -31.21044509
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -1.3052527
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 2.49128397
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -8.19504129
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.50391803
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 12.50382564
