# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : Benzene supercell
Program    : octopus
TestGroups : short-run, periodic_systems
Enabled    : Yes

Processors: 4
Input: 08-benzene_supercell.01-gs.inp

match ; Total k-points   ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 1
match ; Reduced k-points ; GREPFIELD(static/info, 'Number of symmetry-reduced k-points', 6) ; 1
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 47
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;   8
match ; Dipole x     ; GREPFIELD(static/info, '<x>', 3)       ;   0.0
match ; Dipole y     ; GREPFIELD(static/info, '<y>', 3)       ;   0.0
match ; Dipole z     ; GREPFIELD(static/info, '<z>', 3)       ;   0.0

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -39.31167186
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -10.90692500
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; 31.49117774
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -11.17459749
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.62651273
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 30.19471301
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -87.34709738

match ; Eigenvalue  1      ; GREPFIELD(static/info, ' 1   --', 3) ; -0.749736
match ; Eigenvalue  2      ; GREPFIELD(static/info, ' 2   --', 3) ; -0.641172
match ; Eigenvalue  3      ; GREPFIELD(static/info, ' 3   --', 3) ; -0.564650
match ; Eigenvalue  4      ; GREPFIELD(static/info, ' 4   --', 3) ; -0.557280
match ; Eigenvalue  5      ; GREPFIELD(static/info, ' 5   --', 3) ; -0.487222
match ; Eigenvalue  6      ; GREPFIELD(static/info, ' 6   --', 3) ; -0.423668
match ; Eigenvalue  7      ; GREPFIELD(static/info, ' 7   --', 3) ; -0.404147
match ; Eigenvalue  8      ; GREPFIELD(static/info, ' 8   --', 3) ; -0.317089
match ; Eigenvalue  9      ; GREPFIELD(static/info, ' 9   --', 3) ; -0.312911
match ; Eigenvalue 10      ; GREPFIELD(static/info, '10   --', 3) ; -0.255469
match ; Eigenvalue 11      ; GREPFIELD(static/info, '11   --', 3) ; -0.215723
match ; Eigenvalue 12      ; GREPFIELD(static/info, '12   --', 3) ; -0.137084
match ; Eigenvalue 13      ; GREPFIELD(static/info, '13   --', 3) ; -0.135263
match ; Eigenvalue 14      ; GREPFIELD(static/info, '14   --', 3) ; -0.135002
match ; Eigenvalue 15      ; GREPFIELD(static/info, '15   --', 3) ; -0.117047
match ; Eigenvalue 16      ; GREPFIELD(static/info, '16   --', 3) ; 0.002866
