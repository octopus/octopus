# -*- coding: utf-8 mode: shell-script -*-

Test       : Magnons with generalized Bloch theorem
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

Processors : 4

Input      : 21-magnon.01-gs.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 1.10e-08
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -123.76308627
Precision: 7.83e-14
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -78.31680646
Precision: 5.50e-09
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -17.445635915
Precision: 1.10e-08
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 28.41840006
Precision: 5.50e-09
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -15.498991475
Precision: 1.33e-15
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -1.32675244
Precision: 1.10e-08
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 50.75427283
Precision: 2.20e-08
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -107.79320867999999

Precision: 1.00e-15
match ;  k-point 2 (x)   ; GREPFIELD(static/info, '#k =   2', 7) ; 0.0
match ;  k-point 2 (y)   ; GREPFIELD(static/info, '#k =   2', 8) ; 0.5
match ;  k-point 2 (z)   ; GREPFIELD(static/info, '#k =   2', 9) ; 0.0

Precision: 3.35e-15
match ;  Eigenvalue  1   ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -3.346208
match ;  Eigenvalue  2   ; GREPFIELD(static/info, '#k =   2', 3, 2) ; -3.286024
match ;  Eigenvalue  3   ; GREPFIELD(static/info, '#k =   2', 3, 3) ; -1.944945
match ;  Eigenvalue  4   ; GREPFIELD(static/info, '#k =   2', 3, 5) ; -1.930112

Input      : 21-magnon.02-td.inp
Precision : 0.0001
match ; Total magnet. [step  99] ; LINEFIELD(td.general/total_magnetization, -2, 3) ;  -3.191972641737e-03
match ; Total magnet. [step  99] ; LINEFIELD(td.general/total_magnetization, -2, 4) ;  -5.503746916110e-02
match ; Total magnet. [step 100] ; LINEFIELD(td.general/total_magnetization, -1, 3) ;  -3.470772277670e-03
match ; Total magnet. [step 100] ; LINEFIELD(td.general/total_magnetization, -1, 4) ;  -5.468013650141e-02

Util : oct-spin_susceptibility
Input : 21-magnon.02-td.inp

Precision: 1.44e-14
match ;  Spin susceptibilty Re [omega=2.7eV]  ; LINEFIELD(td.general/spin_susceptibility_q001, 30, 2) ; -14.3798
Precision: 5.58e-15
match ;  Spin susceptibilty Im [omega=2.7eV]  ; LINEFIELD(td.general/spin_susceptibility_q001, 30, 3) ; 5.58251
