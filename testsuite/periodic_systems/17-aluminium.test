# -*- coding: utf-8 mode: shell-script -*-

Test       : Aluminium real-time
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

# ground state
Input      : 17-aluminium.01-gs.inp

match ; Total k-points     ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4
match ; Space group        ; GREPFIELD(out, 'Space group', 4) ; 225
match ; No. of symmetries  ; GREPFIELD(out, 'symmetries that can be used', 5)  ;  48

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 1.7e-07
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -9.743506095
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -9.99517178
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; 0.35445004
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 0.012499125
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.7455420349999997
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.696982805
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 3.09381646
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; 1.587874885

Precision: 5.0e-6
match ;  Eigenvalue      [ k = 1, n = 1 ]  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.023446
match ;  Eigenvalue      [ k = 1, n = 2 ]  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -0.023446
match ;  Eigenvalue      [ k = 1, n = 4 ]  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -0.023446
match ;  Eigenvalue      [ k = 1, n = 5 ]  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; 0.210327
match ;  Eigenvalue      [ k = 1, n = 6 ]  ; GREPFIELD(static/info, '#k =   1', 3, 6) ; 0.210327
match ;  Eigenvalue      [ k = 1, n = 7 ]  ; GREPFIELD(static/info, '#k =   1', 3, 7) ; 0.210327
match ;  Eigenvalue      [ k = 1, n = 8 ]  ; GREPFIELD(static/info, '#k =   1', 3, 8) ; 0.210328
match ;  Eigenvalue      [ k = 2, n = 1 ]  ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.023446
match ;  Eigenvalue      [ k = 2, n = 2 ]  ; GREPFIELD(static/info, '#k =   2', 3, 2) ; -0.023446
match ;  Eigenvalue      [ k = 2, n = 3 ]  ; GREPFIELD(static/info, '#k =   2', 3, 3) ; -0.023446
match ;  Eigenvalue      [ k = 2, n = 4 ]  ; GREPFIELD(static/info, '#k =   2', 3, 4) ; -0.023446
match ;  Eigenvalue      [ k = 2, n = 5 ]  ; GREPFIELD(static/info, '#k =   2', 3, 5) ; 0.210327
match ;  Eigenvalue      [ k = 2, n = 6 ]  ; GREPFIELD(static/info, '#k =   2', 3, 6) ; 0.210327
match ;  Eigenvalue      [ k = 2, n = 7 ]  ; GREPFIELD(static/info, '#k =   2', 3, 7) ; 0.210327
match ;  Eigenvalue      [ k = 2, n = 8 ]  ; GREPFIELD(static/info, '#k =   2', 3, 8) ; 0.2103315
match ;  Eigenvalue      [ k = 3, n = 1 ]  ; GREPFIELD(static/info, '#k =   3', 3, 1) ; 0.016441
match ;  Eigenvalue      [ k = 3, n = 2 ]  ; GREPFIELD(static/info, '#k =   3', 3, 2) ; 0.016441
match ;  Eigenvalue      [ k = 3, n = 3 ]  ; GREPFIELD(static/info, '#k =   3', 3, 3) ; 0.016441
match ;  Eigenvalue      [ k = 3, n = 4 ]  ; GREPFIELD(static/info, '#k =   3', 3, 4) ; 0.016441
match ;  Eigenvalue      [ k = 3, n = 5 ]  ; GREPFIELD(static/info, '#k =   3', 3, 5) ; 0.095617
match ;  Eigenvalue      [ k = 3, n = 6 ]  ; GREPFIELD(static/info, '#k =   3', 3, 6) ; 0.095617
match ;  Eigenvalue      [ k = 3, n = 7 ]  ; GREPFIELD(static/info, '#k =   3', 3, 7) ; 0.095617
match ;  Eigenvalue      [ k = 3, n = 8 ]  ; GREPFIELD(static/info, '#k =   3', 3, 8) ; 0.095617
match ;  Eigenvalue      [ k = 4, n = 1 ]  ; GREPFIELD(static/info, '#k =   4', 3, 1) ; 0.016441
match ;  Eigenvalue      [ k = 4, n = 2 ]  ; GREPFIELD(static/info, '#k =   4', 3, 2) ; 0.016441
match ;  Eigenvalue      [ k = 4, n = 3 ]  ; GREPFIELD(static/info, '#k =   4', 3, 3) ; 0.016441
match ;  Eigenvalue      [ k = 4, n = 4 ]  ; GREPFIELD(static/info, '#k =   4', 3, 4) ; 0.016441
match ;  Eigenvalue      [ k = 4, n = 5 ]  ; GREPFIELD(static/info, '#k =   4', 3, 5) ; 0.095617
match ;  Eigenvalue      [ k = 4, n = 6 ]  ; GREPFIELD(static/info, '#k =   4', 3, 6) ; 0.095617
match ;  Eigenvalue      [ k = 4, n = 7 ]  ; GREPFIELD(static/info, '#k =   4', 3, 7) ; 0.095617
match ;  Eigenvalue      [ k = 4, n = 8 ]  ; GREPFIELD(static/info, '#k =   4', 3, 8) ; 0.095617

Input      : 17-aluminium.02-td.inp

Precision: 6.0e-08
match ;  Energy       [step   1]  ; LINEFIELD(td.general/energy, -151, 3) ; -3.861273585058
match ;  Energy       [step  25]  ; LINEFIELD(td.general/energy, -126, 3) ; -3.7302084009509997
match ;  Energy       [step  50]  ; LINEFIELD(td.general/energy, -101, 3) ; -4.0681242174025005
match ;  Energy       [step  75]  ; LINEFIELD(td.general/energy, -76, 3) ; -3.6441697898639998
match ;  Energy       [step 100]  ; LINEFIELD(td.general/energy, -51, 3) ; -4.0979109478755
match ;  Energy       [step 125]  ; LINEFIELD(td.general/energy, -26, 3) ; -3.747503717833
match ;  Energy       [step 150]  ; LINEFIELD(td.general/energy, -1, 3) ; -4.024328243792

Precision: 6e-6
match ;  Gauge field  [step   1]  ; LINEFIELD(td.general/gauge_field, -151, 3) ; 137.035999139
match ;  Gauge field  [step  25]  ; LINEFIELD(td.general/gauge_field, -126, 3) ; -24.163513723945
match ;  Gauge field  [step  50]  ; LINEFIELD(td.general/gauge_field, -101, 3) ; -114.05214552415
match ;  Gauge field  [step  75]  ; LINEFIELD(td.general/gauge_field, -76, 3) ; 56.250646341185
match ;  Gauge field  [step 100]  ; LINEFIELD(td.general/gauge_field, -51, 3) ; 74.17630724132499
match ;  Gauge field  [step 125]  ; LINEFIELD(td.general/gauge_field, -26, 3) ; -74.48832023374
match ;  Gauge field  [step 150]  ; LINEFIELD(td.general/gauge_field, -1, 3) ; -35.465424666355005

Precision: 3.0e-07
match ;  Current      [step   1]  ; LINEFIELD(td.general/total_current, -151, 3) ; 11.674928876245001
match ;  Current      [step  25]  ; LINEFIELD(td.general/total_current, -126, 3) ; -2.5301246965055
match ;  Current      [step  50]  ; LINEFIELD(td.general/total_current, -101, 3) ; -8.3679767018875
match ;  Current      [step  75]  ; LINEFIELD(td.general/total_current, -76, 3) ; 5.299475445245999
match ;  Current      [step 100]  ; LINEFIELD(td.general/total_current, -51, 3) ; 5.1514369975225005
match ;  Current      [step 125]  ; LINEFIELD(td.general/total_current, -26, 3) ; -6.2479833588225
match ;  Current      [step 150]  ; LINEFIELD(td.general/total_current, -1, 3) ; -2.1620704305965


Util : oct-conductivity
Input      : 17-aluminium.03-conductivity.inp

Precision: 0.0001
match ; conductivity  energy 0 ; LINEFIELD(td.general/conductivity, 5, 1) ; 0.000000E+00
Precision: 0.00025
match ; Re cond xx energy 0 ; LINEFIELD(td.general/conductivity, 5, 2) ; 0.3571536411E-02
Precision: 0.0001
match ; Im cond xx energy 0 ; LINEFIELD(td.general/conductivity, 5, 3) ; 0.000000E+00
match ; Re cond yy energy 0 ; LINEFIELD(td.general/conductivity, 5, 4) ; -0.2244449363E-04
match ; Im cond yy energy 0 ; LINEFIELD(td.general/conductivity, 5, 5) ; 0.000000E+00
match ; Re cond zz energy 0 ; LINEFIELD(td.general/conductivity, 5, 6) ; -0.2244449363E-04
match ; Im cond zz energy 0 ; LINEFIELD(td.general/conductivity, 5, 7) ; 0.000000E+00


