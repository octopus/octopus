# -*- coding: utf-8 mode: shell-script -*-

Test       : Aluminium real-time
Program    : octopus
TestGroups : long-run, periodic_systems
Enabled    : Yes

# ground state
Input      : 17-aluminium.01-gs.inp

Precision: 1.00e-16
match ;  Total k-points      ; GREPFIELD(static/info, 'Total number of k-points', 6) ; 4.0
match ;  Space group         ; GREPFIELD(out, 'Space group', 4) ; 225.0
match ;  No. of symmetries   ; GREPFIELD(out, 'symmetries that can be used', 5) ; 48.0
match ;  SCF convergence     ; GREPCOUNT(static/info, 'SCF converged') ; 1.0

Precision: 9.63e-15
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -9.62876758
Precision: 1.00e-14
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -9.99517178
Precision: 4.70e-16
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; 0.4695873
Precision: 1.31e-17
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 0.01305015
Precision: 3.75e-15
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.74590707
Precision: 6.97e-16
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.6970093
Precision: 3.22e-15
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 3.22476306
Precision: 1.57e-15
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; 1.57150746

Precision: 5.97e-16
match ;  Eigenvalue      [ k = 1, n = 1 ]  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.023422
match ;  Eigenvalue      [ k = 1, n = 2 ]  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; -0.023422
match ;  Eigenvalue      [ k = 1, n = 4 ]  ; GREPFIELD(static/info, '#k =   1', 3, 4) ; -0.023419
match ;  Eigenvalue      [ k = 1, n = 5 ]  ; GREPFIELD(static/info, '#k =   1', 3, 5) ; 0.210412
match ;  Eigenvalue      [ k = 1, n = 6 ]  ; GREPFIELD(static/info, '#k =   1', 3, 6) ; 0.210419
match ;  Eigenvalue      [ k = 1, n = 7 ]  ; GREPFIELD(static/info, '#k =   1', 3, 7) ; 0.210419
match ;  Eigenvalue      [ k = 1, n = 8 ]  ; GREPFIELD(static/info, '#k =   1', 3, 8) ; 0.561939
match ;  Eigenvalue      [ k = 2, n = 1 ]  ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.023422
match ;  Eigenvalue      [ k = 2, n = 2 ]  ; GREPFIELD(static/info, '#k =   2', 3, 2) ; -0.023422
match ;  Eigenvalue      [ k = 2, n = 3 ]  ; GREPFIELD(static/info, '#k =   2', 3, 3) ; -0.023419
match ;  Eigenvalue      [ k = 2, n = 4 ]  ; GREPFIELD(static/info, '#k =   2', 3, 4) ; -0.023419
match ;  Eigenvalue      [ k = 2, n = 5 ]  ; GREPFIELD(static/info, '#k =   2', 3, 5) ; 0.210412
match ;  Eigenvalue      [ k = 2, n = 6 ]  ; GREPFIELD(static/info, '#k =   2', 3, 6) ; 0.210419
match ;  Eigenvalue      [ k = 2, n = 7 ]  ; GREPFIELD(static/info, '#k =   2', 3, 7) ; 0.210419
match ;  Eigenvalue      [ k = 2, n = 8 ]  ; GREPFIELD(static/info, '#k =   2', 3, 8) ; 0.561939
match ;  Eigenvalue      [ k = 3, n = 1 ]  ; GREPFIELD(static/info, '#k =   3', 3, 1) ; 0.016464
match ;  Eigenvalue      [ k = 3, n = 2 ]  ; GREPFIELD(static/info, '#k =   3', 3, 2) ; 0.016464
match ;  Eigenvalue      [ k = 3, n = 3 ]  ; GREPFIELD(static/info, '#k =   3', 3, 3) ; 0.016473
match ;  Eigenvalue      [ k = 3, n = 4 ]  ; GREPFIELD(static/info, '#k =   3', 3, 4) ; 0.016473
match ;  Eigenvalue      [ k = 3, n = 5 ]  ; GREPFIELD(static/info, '#k =   3', 3, 5) ; 0.095649
match ;  Eigenvalue      [ k = 3, n = 6 ]  ; GREPFIELD(static/info, '#k =   3', 3, 6) ; 0.095664
match ;  Eigenvalue      [ k = 3, n = 7 ]  ; GREPFIELD(static/info, '#k =   3', 3, 7) ; 0.095664
match ;  Eigenvalue      [ k = 3, n = 8 ]  ; GREPFIELD(static/info, '#k =   3', 3, 8) ; 0.59725
match ;  Eigenvalue      [ k = 4, n = 1 ]  ; GREPFIELD(static/info, '#k =   4', 3, 1) ; 0.016464
match ;  Eigenvalue      [ k = 4, n = 2 ]  ; GREPFIELD(static/info, '#k =   4', 3, 2) ; 0.016464
match ;  Eigenvalue      [ k = 4, n = 3 ]  ; GREPFIELD(static/info, '#k =   4', 3, 3) ; 0.016473
match ;  Eigenvalue      [ k = 4, n = 4 ]  ; GREPFIELD(static/info, '#k =   4', 3, 4) ; 0.016473
match ;  Eigenvalue      [ k = 4, n = 5 ]  ; GREPFIELD(static/info, '#k =   4', 3, 5) ; 0.095649
match ;  Eigenvalue      [ k = 4, n = 6 ]  ; GREPFIELD(static/info, '#k =   4', 3, 6) ; 0.095664
match ;  Eigenvalue      [ k = 4, n = 7 ]  ; GREPFIELD(static/info, '#k =   4', 3, 7) ; 0.095664
match ;  Eigenvalue      [ k = 4, n = 8 ]  ; GREPFIELD(static/info, '#k =   4', 3, 8) ; 0.59725


Input      : 17-aluminium.02-td.inp

Precision: 7.43e-11
match ;  Energy       [step   1]  ; LINEFIELD(td.general/energy, -151, 3) ; -3.7495469157825
match ;  Energy       [step  25]  ; LINEFIELD(td.general/energy, -126, 3) ; -3.615039834191
match ;  Energy       [step  50]  ; LINEFIELD(td.general/energy, -101, 3) ; -3.9542542005215
match ;  Energy       [step  75]  ; LINEFIELD(td.general/energy, -76, 3) ; -3.5407639387885
match ;  Energy       [step 100]  ; LINEFIELD(td.general/energy, -51, 3) ; -3.9783375004490003
match ;  Energy       [step 125]  ; LINEFIELD(td.general/energy, -26, 3) ; -3.64408953324
match ;  Energy       [step 150]  ; LINEFIELD(td.general/energy, -1, 3) ; -3.9017266887334996

Precision: 3.44e-09
match ;  Gauge field  [step   1]  ; LINEFIELD(td.general/gauge_field, -151, 3) ; 137.035999139
match ;  Gauge field  [step  25]  ; LINEFIELD(td.general/gauge_field, -126, 3) ; -23.728269660055
match ;  Gauge field  [step  50]  ; LINEFIELD(td.general/gauge_field, -101, 3) ; -112.38354412525001
match ;  Gauge field  [step  75]  ; LINEFIELD(td.general/gauge_field, -76, 3) ; 54.7136542116
match ;  Gauge field  [step 100]  ; LINEFIELD(td.general/gauge_field, -51, 3) ; 69.919518533065
match ;  Gauge field  [step 125]  ; LINEFIELD(td.general/gauge_field, -26, 3) ; -74.24486057868
match ;  Gauge field  [step 150]  ; LINEFIELD(td.general/gauge_field, -1, 3) ; -31.299136599195002

Precision: 2.66e-10
match ;  Current      [step   1]  ; LINEFIELD(td.general/total_current, -151, 3) ; 11.683210375545
match ;  Current      [step  25]  ; LINEFIELD(td.general/total_current, -126, 3) ; -2.586714416915
match ;  Current      [step  50]  ; LINEFIELD(td.general/total_current, -101, 3) ; -8.238292123926001
match ;  Current      [step  75]  ; LINEFIELD(td.general/total_current, -76, 3) ; 5.2571966597765005
match ;  Current      [step 100]  ; LINEFIELD(td.general/total_current, -51, 3) ; 4.962482048279
match ;  Current      [step 125]  ; LINEFIELD(td.general/total_current, -26, 3) ; -6.2732081711414995
match ;  Current      [step 150]  ; LINEFIELD(td.general/total_current, -1, 3) ; -1.901006787562


Util : oct-conductivity
Input      : 17-aluminium.03-conductivity.inp

Precision: 1.0e-16
match ;  conductivity  energy 0  ; LINEFIELD(td.general/conductivity, 5, 1) ; 0.0
match ;  Re cond xx energy 0  ; LINEFIELD(td.general/conductivity, 5, 2) ; 0.003577023
match ;  Im cond xx energy 0  ; LINEFIELD(td.general/conductivity, 5, 3) ; 0.0
match ;  Re cond yy energy 0  ; LINEFIELD(td.general/conductivity, 5, 4) ; -0.0001230902302
match ;  Im cond yy energy 0  ; LINEFIELD(td.general/conductivity, 5, 5) ; 0.0
match ;  Re cond zz energy 0  ; LINEFIELD(td.general/conductivity, 5, 6) ; -0.0001230902302
match ;  Im cond zz energy 0  ; LINEFIELD(td.general/conductivity, 5, 7) ; 0.0
