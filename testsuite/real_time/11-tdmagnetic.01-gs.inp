CalculationMode = gs
FromScratch = yes
Dimensions = 2

omega0 = 1.0
dist = 3.0
%Species
"qd" | species_user_defined | potential_formula | "0.5*omega0*min((x-dist/2)^2+y^2,(x+dist/2)^2+y^2-0.02)" | valence | 1
%

%Coordinates
"qd" | 0 | 0 | 0 
%

BoxShape = sphere
Radius = 9.5
Spacing = 0.23

DerivativesOrder = 4

TheoryLevel = independent_particles

MaximumIter = 3
ConvRelDens = 1e-4

EigenSolver = plan
EigenSolverTolerance = 1e-7
EigenSolverMaxIter = 5000

magfield = 3.0

# P_c is the speed of light in atomic units.
P_c = 137.036
%StaticMagneticField
0 | 0 | magfield*P_c
%




StatesPack = yes
