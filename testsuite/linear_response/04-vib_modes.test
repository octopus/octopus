# -*- coding: utf-8 mode: shell-script -*-

Test       : LiH vibrational frequency
Program    : octopus
TestGroups : short-run, linear_response
Enabled    : Yes

Processors : 4

Input: 04-vib_modes.01-ground_state.inp
match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
match ; Total Energy   ; GREPFIELD(static/info, 'Total       =', 3) ; -0.76459856
match ; Eigenvalues    ; GREPFIELD(static/info, '1   --', 3) ;        -0.141240
Precision : 0.00011
match ; Dipole Moment  ; GREPFIELD(static/info, '<z>', 3) ;            2.24560

Input: 04-vib_modes.02-vib_modes.inp
Precision: 3.08e-05
match ;   Vibrational Frequency     ; LINEFIELD(vib_modes/normal_frequencies_lr, 1, 2) ; 1420.92513578
Precision: 3.05e-16
match ;   Infrared Intensity        ; LINEFIELD(vib_modes/infrared, 2, 5) ; 0.30478569
Precision: 2.35e-08
match ;   Dynamical Matrix Entry    ; LINEFIELD(vib_modes/dynamical_matrix_lr, 2, 5) ; -0.018043337887

Input: 04-vib_modes.022-vib_modes_restart.inp
Precision: 3.08e-05
match ;   Vibrational Frequency     ; LINEFIELD(vib_modes/normal_frequencies_lr, 1, 2) ; 1420.92513578
Precision: 3.05e-16
match ;   Infrared Intensity        ; LINEFIELD(vib_modes/infrared, 2, 5) ; 0.30478569
Precision: 2.35e-08
match ;   Dynamical Matrix Entry    ; LINEFIELD(vib_modes/dynamical_matrix_lr, 2, 5) ; -0.018043337887

