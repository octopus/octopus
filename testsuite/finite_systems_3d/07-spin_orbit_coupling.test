  match ; Eigenvalue 15 ; GREPFIELD(static/info, '15   --', 3) ; -14.214619
  
  Precision : 0.001
  match ; <Sz>        1 ; GREPFIELD(static/info, ' 1   --', 7) ;  0.5000
