# -*- coding: utf-8 mode: shell-script -*-

Test       : Spin-orbit coupling
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : Yes


Input: 07-spin_orbit_coupling.01-hgh.inp
if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence ; GREPCOUNT(static/info, 'SCF \*not\* converged') ; 1

  Precision: 3.e-14
  match ;  Eigenvalue  1  ; GREPFIELD(static/info, ' 1   --', 3) ; -28.179039
  match ;  Eigenvalue  3  ; GREPFIELD(static/info, ' 3   --', 3) ; -27.237103
  match ;  Eigenvalue  5  ; GREPFIELD(static/info, ' 5   --', 3) ; -17.893173
  match ;  Eigenvalue  7  ; GREPFIELD(static/info, ' 7   --', 3) ; -16.377643
  match ;  Eigenvalue  9  ; GREPFIELD(static/info, ' 9   --', 3) ; -15.306479
  match ;  Eigenvalue 11  ; GREPFIELD(static/info, '11   --', 3) ; -15.284568
  match ;  Eigenvalue 13  ; GREPFIELD(static/info, '13   --', 3) ; -14.468441
  match ;  Eigenvalue 15  ; GREPFIELD(static/info, '15   --', 3) ; -14.203057

  Precision: 5.00e-16
  match ;  <Sz>        1  ; GREPFIELD(static/info, ' 1   --', 7) ; 0.5
  match ;  <Sz>        3  ; GREPFIELD(static/info, ' 3   --', 7) ; 0.5
  match ;  <Sz>        5  ; GREPFIELD(static/info, ' 5   --', 7) ; 0.4753
  match ;  <Sz>        7  ; GREPFIELD(static/info, ' 7   --', 7) ; 0.1391
  match ;  <Sz>        9  ; GREPFIELD(static/info, ' 9   --', 7) ; -0.0865
  match ;  <Sz>       11  ; GREPFIELD(static/info, '11   --', 7) ; 0.1394
  match ;  <Sz>       13  ; GREPFIELD(static/info, '13   --', 7) ; -0.1142
  match ;  <Sz>       15  ; GREPFIELD(static/info, '15   --', 7) ; 0.4471

  Precision: 9.49e-16
  match ;  Force          ; GREPFIELD(static/info, 'Forces on the ions', 3, 2) ; -0.948705
endif

Precision : default
  
ExtraFile : 07-spin_orbit_coupling.02-xe-pseudo.upf
Input: 07-spin_orbit_coupling.02-rkb.inp

if(available cuda); then
