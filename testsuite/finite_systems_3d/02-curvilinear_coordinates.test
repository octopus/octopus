# -*- coding: utf-8 mode: shell-script -*-

Test       : Curvilinear Coordinates
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : Yes

Processors : 1
# curvilinear not compatible with domain parallelization
# there is only one state, so we cannot use state parallelization
Input: 02-curvilinear_coordinates.01-gygi.inp

match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1
Precision: 1.37e-05
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -2.8681476200000002
Precision: 1.00e-15
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; 0.0
Precision: 1.55e-05
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -1.1522392149999998
Precision: 3.18e-06
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 2.021299045
Precision: 9.01e-06
match ;  Int[n*v_xc]         ; GREPFIELD(static/info, 'Int\[n\*v_xc\] =', 3) ; -1.28991726
Precision: 6.51e-06
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.873090275
Precision: 3.19e-07
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.11143892
Precision: 3.87e-05
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 2.81810045
Precision: 4.71e-05
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -6.72301637

Precision: 7.70e-06
match ;  Eigenvalue          ; GREPFIELD(static/info, '1   --', 3) ; -0.57612

