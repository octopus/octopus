# -*- coding: utf-8 mode: shell-script -*-

Test       : Slater Exchange for Cl atom
Program    : octopus
TestGroups : long-run, finite_systems_3d
Enabled    : Yes

Input: 35-slater_x.01-gs.inp
match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
Precision: 0.00011
match ; Total energy     ; GREPFIELD(static/info, 'Total       =', 3) ; -397.18884896
match ; Eigenvalue   [1] ; GREPFIELD(static/info, '1   --', 3) ; -20.361195
match ; Sx           [1] ; GREPFIELD(static/info, '1   --', 5) ; -0.1178
match ; Sy           [1] ; GREPFIELD(static/info, '1   --', 6) ;  0.0000
match ; Sz           [1] ; GREPFIELD(static/info, '1   --', 7) ;  0.4859

match ; Eigenvalue   [4] ; GREPFIELD(static/info, '4   --', 3) ; -8.701239
match ; Sx           [4] ; GREPFIELD(static/info, '4   --', 5) ; -0.1175
match ; Sy           [4] ; GREPFIELD(static/info, '4   --', 6) ;  0.0000
match ; Sz           [4] ; GREPFIELD(static/info, '4   --', 7) ;  0.4849

match ; Eigenvalue   [6] ; GREPFIELD(static/info, '6   --', 3) ; -6.997120
match ; Sx           [6] ; GREPFIELD(static/info, '6   --', 5) ;  0.1175
match ; Sy           [6] ; GREPFIELD(static/info, '6   --', 6) ;  0.0000
match ; Sz           [6] ; GREPFIELD(static/info, '6   --', 7) ; -0.4849

