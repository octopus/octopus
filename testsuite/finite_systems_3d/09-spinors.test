# -*- coding: utf-8 mode: shell-script -*-

Test     : Spinors
Program    : octopus
TestGroups  : short-run, finite_systems_3d
Enabled  : Yes

Input: 09-spinors.01-ground_state.inp
match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
match ; Total energy ; GREPFIELD(static/info, 'Total       =', 3) ; -0.17669593
match ; External energy ; GREPFIELD(static/info, 'External    =', 3) ; -0.29125118
match ; Eigenvalue   ; GREPFIELD(static/info, '1   --', 3) ; -0.085321
Precision: 0.00011
match ; <Sx>         ; GREPFIELD(static/info, '1   --', 5) ; 0.1929
match ; <Sy>         ; GREPFIELD(static/info, '1   --', 6) ; 0.1929
match ; <Sz>         ; GREPFIELD(static/info, '1   --', 7) ; 0.4190
Precision: 0.00023
match ; mx           ; GREPFIELD(static/info, 'Local Magnetic Moments', 3, 2) ; 0.385780
match ; my           ; GREPFIELD(static/info, 'Local Magnetic Moments', 4, 2) ; 0.385780
match ; mz           ; GREPFIELD(static/info, 'Local Magnetic Moments', 5, 2) ; 0.838062
Precision: 0.0001

Input: 09-spinors.02-rmmdiis.inp
match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1
Precision: 1.77e-16
match ;  Total energy  ; GREPFIELD(static/info, 'Total       =', 3) ; -0.17669589
Precision: 2.91e-16
match ;  External energy  ; GREPFIELD(static/info, 'External    =', 3) ; -0.29125116
Precision: 8.53e-17
match ;  Eigenvalue    ; GREPFIELD(static/info, '1   --', 3) ; -0.085321
Precision: 1.99e-16
match ;  <Sx>          ; GREPFIELD(static/info, '1   --', 5) ; 0.1988
Precision: 1.99e-16
match ;  <Sy>          ; GREPFIELD(static/info, '1   --', 6) ; 0.1988
Precision: 4.13e-16
match ;  <Sz>          ; GREPFIELD(static/info, '1   --', 7) ; 0.4134
Precision: 5.50e-07
match ;  mx            ; GREPFIELD(static/info, 'Local Magnetic Moments', 3, 2) ; 0.3976725
Precision: 5.50e-07
match ;  my            ; GREPFIELD(static/info, 'Local Magnetic Moments', 4, 2) ; 0.3976725
Precision: 5.50e-07
match ;  mz            ; GREPFIELD(static/info, 'Local Magnetic Moments', 5, 2) ; 0.8268694999999999
# This differs significantly from the initial magnetization.
# It is due to the small radius and large spacing.

Input: 09-spinors.03-rmmdiis_extra_states.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 1.77e-16
match ;  Total energy  ; GREPFIELD(static/info, 'Total       =', 3) ; -0.17669589
Precision: 5.50e-09
match ;  External energy  ; GREPFIELD(static/info, 'External    =', 3) ; -0.291251155
Precision: 8.53e-17
match ;  Eigenvalue    ; GREPFIELD(static/info, '1   --', 3) ; -0.085321
Precision: 8.60e-17
match ;  <Sx>          ; GREPFIELD(static/info, '1   --', 5) ; 0.086
Precision: 8.60e-17
match ;  <Sy>          ; GREPFIELD(static/info, '1   --', 6) ; 0.086
Precision: 4.85e-16
match ;  <Sz>          ; GREPFIELD(static/info, '1   --', 7) ; 0.485
Precision: 1.72e-16
match ;  mx            ; GREPFIELD(static/info, 'Local Magnetic Moments', 3, 2) ; 0.172065
Precision: 1.72e-16
match ;  my            ; GREPFIELD(static/info, 'Local Magnetic Moments', 4, 2) ; 0.172065
Precision: 9.70e-16
match ;  mz            ; GREPFIELD(static/info, 'Local Magnetic Moments', 5, 2) ; 0.969942
