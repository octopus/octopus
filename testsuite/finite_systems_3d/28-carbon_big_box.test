# -*- coding: utf-8 mode: shell-script -*-

Test       : Big box
Program    : octopus
TestGroups : short-run, finite_systems_3d
Enabled    : Yes

Input: 28-carbon_big_box.01-ground_state.inp
Precision: 0.0001
match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 2.69e-07
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -10.164901754999999
Precision: 1.00e-15
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; 0.0
Precision: 3.24e-07
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -9.217878445
Precision: 1.21e-07
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 1.06350681
Precision: 7.70e-08
match ;  Int[n*v_xc]         ; GREPFIELD(static/info, 'Int\[n\*v_xc\] =', 3) ; -0.4993593
Precision: 5.50e-08
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.30459747000000004
Precision: 5.50e-09
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.078278345
Precision: 6.05e-08
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 0.147327885
Precision: 1.26e-07
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -10.992847835
Precision: 5.50e-07
match ;  Eigenvalue   [1up]  ; GREPFIELD(static/info, '1   up', 3) ; -4.5472085
Precision: 1.00e-15
match ;  Occupation   [1up]  ; GREPFIELD(static/info, '1   up', 4) ; 1.0
Precision: 5.50e-07
match ;  Eigenvalue [4down]  ; GREPFIELD(static/info, '4   dn', 3) ; -0.0359945
Precision: 1.00e-15
match ;  Occupation [4down]  ; GREPFIELD(static/info, '4   dn', 4) ; 0.0
