# -*- coding: utf-8 mode: shell-script -*-

Test       : Static magnetic field
Program    : octopus
TestGroups : long-run, finite_systems_3d
Enabled    : Yes

Input: 03-magnetic.01-gs-unpolarized.inp
match ;  SCF convergence   ; GREPCOUNT(static/info, 'SCF converged') ; 1.0
match ;  Eigenvalue  1     ; GREPFIELD(static/info, '  1   --', 3) ; -0.248425
Precision: 5.50e-07
match ;  Eigenvalue  2     ; GREPFIELD(static/info, '  2   --', 3) ; -0.0569385
Precision: 8.25e-08
match ;  Eigenvalue  3     ; GREPFIELD(static/info, '  3   --', 3) ; -0.04861
match ;  Eigenvalue  4     ; GREPFIELD(static/info, '  4   --', 3) ; -0.039065
match ;  Total energy      ; GREPFIELD(static/info, 'Total       =', 3) ; -1.912211975
Precision: 7.70e-08
match ;  Free energy       ; GREPFIELD(static/info, 'Free        =', 3) ; -1.93735519
match ;  Fermi energy      ; GREPFIELD(static/info, 'Fermi energy =', 4) ; -0.066015

Input: 03-magnetic.02-td-unpolarized.inp
Precision: 8.19e-11
match ;  Energy [step  1]  ; LINEFIELD(td.general/energy, -21, 3) ; -1.9122118917405
Precision: 3.37e-10
match ;  Energy [step  5]  ; LINEFIELD(td.general/energy, -16, 3) ; -1.8975681556869999
Precision: 3.37e-10
match ;  Energy [step 10]  ; LINEFIELD(td.general/energy, -11, 3) ; -1.8975681356890002
Precision: 3.37e-10
match ;  Energy [step 15]  ; LINEFIELD(td.general/energy, -6, 3) ; -1.8975686324
Precision: 3.37e-10
match ;  Energy [step 20]  ; LINEFIELD(td.general/energy, -1, 3) ; -1.8975686638620002

