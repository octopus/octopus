# -*- coding: utf-8 mode: shell-script -*-

Test       : Tkatchenko-Scheffler van der Waals correction (self-consistent) in solids 
Program    : octopus
TestGroups : functionals, long-run
Enabled    : Yes

Processors : 3
Input      : 12-vdw_solid_c6.01-gs_diamond.inp 

match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 1.13e-14
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -11.31904289
Precision: 6.09e-16
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.6087095
Precision: 1.07e-15
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 1.07101008
Precision: 1.07e-14
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -10.73490075
Precision: 3.24e-15
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.23975896
Precision: 3.54e-16
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.35381238
Precision: 9.01e-15
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 9.01345128
Precision: 7.06e-15
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; -7.06076456
Precision: 1.44e-17
match ;  van der Waals energy  ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.01436882
Precision: 9.93e-20
match ;  Force C1 (x)    ; GREPFIELD(static/info, '1         C', 3) ; -9.9265e-05
Precision: 9.93e-20
match ;  Force C1 (y)    ; GREPFIELD(static/info, '1         C', 4) ; -9.93301e-05
Precision: 1.42e-20
match ;  Force C1 (z)    ; GREPFIELD(static/info, '1         C', 5) ; -1.42147e-05
Precision: 1.06e-19
match ;  Force C2 (x)    ; GREPFIELD(static/info, '2         C', 3) ; 0.000105739
Precision: 1.06e-19
match ;  Force C2 (y)    ; GREPFIELD(static/info, '2         C', 4) ; 0.000105793
Precision: 1.67e-20
match ;  Force C2 (z)    ; GREPFIELD(static/info, '2         C', 5) ; 1.6692e-05
Precision: 3.67e-14
match ;  C6 eff  C1-C1    ; GREPFIELD(static/c6ab_eff, '1    1', 3) ; 36.7188
Precision: 3.68e-14
match ;  C6 eff  C1-C2    ; GREPFIELD(static/c6ab_eff, '1    2', 3) ; 36.7974
Precision: 3.68e-14
match ;  C6 eff  C2-C1    ; GREPFIELD(static/c6ab_eff, '2    1', 3) ; 36.7974
Precision: 3.69e-14
match ;  C6 eff  C2-C2    ; GREPFIELD(static/c6ab_eff, '2    2', 3) ; 36.8761


Processors : 2
Input      : 12-vdw_solid_c6.02-gs_graphene.inp
if(available cuda); then
  match ; Error match ; GREPCOUNT(err, 'Calculation will not be continued.') ; 1
else
  match ; SCF convergence    ; GREPCOUNT(static/info, 'SCF converged') ; 1

  Precision: 4.68e-07
  match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -11.332406565
  Precision: 3.30e-07
  match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -3.71113397
  Precision: 1.37e-07
  match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; -11.110963555
  Precision: 1.98e-14
  match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -19.80012852
  Precision: 2.20e-08
  match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.20239797
  Precision: 3.31e-16
  match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.33134724
  Precision: 9.90e-08
  match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 8.6318936
  Precision: 2.20e-07
  match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; 14.483775529999999
  Precision: 3.36e-18
  match ;  van der Waals energy  ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.00336062
  Precision: 6.60e-09
  match ;  Force C1 (x)    ; GREPFIELD(static/info, '1         C', 3) ; -0.000276569
  Precision: 3.30e-09
  match ;  Force C1 (y)    ; GREPFIELD(static/info, '1         C', 4) ; 0.000159859
  Precision: 1.19e-10
  match ;  Force C1 (z)    ; GREPFIELD(static/info, '1         C', 5) ; -7.68983e-11
  Precision: 7.15e-09
  match ;  Force C2 (x)    ; GREPFIELD(static/info, '2         C', 3) ; -0.0002382175
  Precision: 3.85e-09
  match ;  Force C2 (y)    ; GREPFIELD(static/info, '2         C', 4) ; 0.0001431985
  Precision: 1.91e-11
  match ;  Force C2 (z)    ; GREPFIELD(static/info, '2         C', 5) ; 3.237265e-11
  Precision: 3.29e-14
  match ;  C6 eff  C1-C1    ; GREPFIELD(static/c6ab_eff, '1    1', 3) ; 32.921
  Precision: 3.29e-14
  match ;  C6 eff  C1-C2    ; GREPFIELD(static/c6ab_eff, '1    2', 3) ; 32.9401
  Precision: 3.29e-14
  match ;  C6 eff  C2-C1    ; GREPFIELD(static/c6ab_eff, '2    1', 3) ; 32.9401
  Precision: 3.30e-14
  match ;  C6 eff  C2-C2    ; GREPFIELD(static/c6ab_eff, '2    2', 3) ; 32.9591
endif
