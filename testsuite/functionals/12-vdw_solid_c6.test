# -*- coding: utf-8 mode: shell-script -*-

Test       : Tkatchenko-Scheffler van der Waals correction (self-consistent) in solids 
Program    : octopus
TestGroups : functionals, long-run, VDW_test
Enabled    : Yes

Processors : 3
Input      : 12-vdw_solid_c6.01-gs_diamond.inp 

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -11.32048941
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.55735014
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ;  1.05336267
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -10.73490075
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.23177615
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.35466595
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  8.98734498
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; -7.02336128
match ; van der Waals energy ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.01443613

match ; Force C1 (x)   ; GREPFIELD(static/info, '1         C', 3) ; -0.000774
match ; Force C1 (y)   ; GREPFIELD(static/info, '1         C', 4) ; -0.000774
match ; Force C1 (z)   ; GREPFIELD(static/info, '1         C', 5) ; -0.000867
match ; Force C2 (x)   ; GREPFIELD(static/info, '2         C', 3) ;  0.000773
match ; Force C2 (y)   ; GREPFIELD(static/info, '2         C', 4) ;  0.000774
match ; Force C2 (z)   ; GREPFIELD(static/info, '2         C', 5) ;  0.000867

match ; C6 eff  C1-C1   ; GREPFIELD(static/c6ab_eff, '1        1', 3) ; 3.70754486E+01
match ; C6 eff  C1-C2   ; GREPFIELD(static/c6ab_eff, '1        2', 3) ; 3.70687179E+01
match ; C6 eff  C2-C1   ; GREPFIELD(static/c6ab_eff, '2        1', 3) ; 3.70687179E+01
match ; C6 eff  C2-C2   ; GREPFIELD(static/c6ab_eff, '2        2', 3) ; 3.70619884E+01


Processors : 2
Input      : 12-vdw_solid_c6.02-gs_graphene.inp

match ; Total energy       ; GREPFIELD(static/info, 'Total       =', 3) ; -11.33217565
match ; Eigenvalues sum    ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -3.70853634
match ; Hartree energy     ; GREPFIELD(static/info, 'Hartree     =', 3) ; -11.11134461
match ; Ion-ion energy     ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -19.80012852
match ; Exchange energy    ; GREPFIELD(static/info, 'Exchange    =', 3) ; -3.20223611
match ; Correlation energy ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.33136593
match ; Kinetic energy     ; GREPFIELD(static/info, 'Kinetic     =', 3) ;  8.63088705
match ; External energy    ; GREPFIELD(static/info, 'External    =', 3) ; 14.48503057
match ; van der Waals energy ; GREPFIELD(static/info, 'vanderWaals =', 3) ; -0.00335712

match ; Force C1 (x)   ; GREPFIELD(static/info, '1         C', 3) ; -0.000790
match ; Force C1 (y)   ; GREPFIELD(static/info, '1         C', 4) ;  0.000328
match ; Force C1 (z)   ; GREPFIELD(static/info, '1         C', 5) ;  0.000000
match ; Force C2 (x)   ; GREPFIELD(static/info, '2         C', 3) ;  0.000179
match ; Force C2 (y)   ; GREPFIELD(static/info, '2         C', 4) ;  0.000039
match ; Force C2 (z)   ; GREPFIELD(static/info, '2         C', 5) ;  0.000000

match ; C6 eff  C1-C1   ; GREPFIELD(static/c6ab_eff, '1        1', 3) ; 3.28943729E+01
match ; C6 eff  C1-C2   ; GREPFIELD(static/c6ab_eff, '1        2', 3) ; 3.28876466E+01
match ; C6 eff  C2-C1   ; GREPFIELD(static/c6ab_eff, '2        1', 3) ; 3.28876466E+01
match ; C6 eff  C2-C2   ; GREPFIELD(static/c6ab_eff, '2        2', 3) ; 3.28809216E+01

