# -*- coding: utf-8 mode: shell-script -*-
# $Id$

Test       : RDMFT
Program    : octopus
TestGroups : long-run, functionals
Enabled    : Yes

Processors: 4

Input: 06-rdmft.01-oep.inp
match ; OEP energy                    ; GREP(static/info, 'Total       =', 20)   ; -1.23959074
match ; OEP eigenvalue                ; GREP(static/info, '  1   --', 13) ;   -0.633895

Input: 06-rdmft.02-gs.inp
Precision : 1e-5
match ; RDMFT energy after occupation numbers minimization            ; GREP(out, 'occ minim = ',     30) ; -1.15657
match ; RDMFT energy after orbital minimization                       ; GREP(out, 'orbital minim = ', 34) ; -1.12936 
