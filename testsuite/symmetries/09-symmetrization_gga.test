# -*- coding: utf-8 mode: shell-script -*-

Test       : Real space symmetrization with GGA
Program    : octopus
TestGroups : long-run, periodic_systems, symmetries
Enabled    : Yes

# Here the reference values should be copied to the calculation without symmetries
Input : 09-symmetrization_gga.01-spg143_nosym.inp

match ; SCF convergence ; GREPCOUNT(static/info, 'SCF converged') ; 1

Precision: 1.54e-07
match ;  Total energy        ; GREPFIELD(static/info, 'Total       =', 3) ; -0.14124755
Precision: 1.32e-16
match ;  Ion-ion energy      ; GREPFIELD(static/info, 'Ion-ion     =', 3) ; -0.13210182
Precision: 1.65e-07
match ;  Eigenvalues sum     ; GREPFIELD(static/info, 'Eigenvalues =', 3) ; -0.13551012
Precision: 4.40e-08
match ;  Hartree energy      ; GREPFIELD(static/info, 'Hartree     =', 3) ; 0.03169699
Precision: 4.39e-16
match ;  Exchange energy     ; GREPFIELD(static/info, 'Exchange    =', 3) ; -0.43889066
Precision: 4.40e-08
match ;  Correlation energy  ; GREPFIELD(static/info, 'Correlation =', 3) ; -0.0907665
Precision: 1.98e-07
match ;  Kinetic energy      ; GREPFIELD(static/info, 'Kinetic     =', 3) ; 0.37545558
Precision: 1.54e-07
match ;  External energy     ; GREPFIELD(static/info, 'External    =', 3) ; 0.11335886
Precision: 1.37e-16
match ;  Eigenvalue [  k=1, n=1   ]  ; GREPFIELD(static/info, '#k =   1', 3, 1) ; -0.136963
Precision: 7.29e-17
match ;  Eigenvalue [  k=1, n=2   ]  ; GREPFIELD(static/info, '#k =   1', 3, 2) ; 0.072893
Precision: 1.37e-16
match ;  Eigenvalue [  k=1, n=3   ]  ; GREPFIELD(static/info, '#k =   1', 3, 3) ; 0.136691
Precision: 1.12e-16
match ;  Eigenvalue [  k=2, n=1   ]  ; GREPFIELD(static/info, '#k =   2', 3, 1) ; -0.11181
Precision: 8.48e-17
match ;  Eigenvalue [  k=2, n=2   ]  ; GREPFIELD(static/info, '#k =   2', 3, 2) ; 0.084804
Precision: 9.73e-17
match ;  Eigenvalue [  k=2, n=3   ]  ; GREPFIELD(static/info, '#k =   2', 3, 3) ; 0.097262
Precision: 1.10e-06
match ;  Force 1 (x)    ; GREPFIELD(static/info, '1        Na', 3) ; 0.17158
Precision: 3.30e-06
match ;  Force 1 (y)    ; GREPFIELD(static/info, '1        Na', 4) ; 0.235951
Precision: 1.10e-06
match ;  Force 2 (x)    ; GREPFIELD(static/info, '2        Na', 3) ; 0.118549
Precision: 2.20e-06
match ;  Force 2 (y)    ; GREPFIELD(static/info, '2        Na', 4) ; -0.266564
Precision: 2.98e-08
match ;  Force 2 (z)    ; GREPFIELD(static/info, '2        Na', 5) ; 9.22864e-06
Precision: 2.20e-06
match ;  Force 3 (x)    ; GREPFIELD(static/info, '3        Na', 3) ; -0.290125
Precision: 2.53e-06
match ;  Force 3 (y)    ; GREPFIELD(static/info, '3        Na', 4) ; 0.0306201
Precision: 1.73e-08
match ;  Force 3 (z)    ; GREPFIELD(static/info, '3        Na', 5) ; 9.23995e-06
Precision: 1.00e-15
match ;  Partial charge  1  ; GREPFIELD(static/info, 'Partial ionic charges', 3, 2) ; 1.0
Precision: 1.00e-15
match ;  Partial charge  2  ; GREPFIELD(static/info, 'Partial ionic charges', 3, 3) ; 1.0
Precision: 2.02e-07
match ;  Density value 1    ; LINEFIELD(static/density.y=0\,z=0, 2, 2) ; 0.00974954513287314
Precision: 5.92e-07
match ;  Density value 2    ; LINEFIELD(static/density.y=0\,z=0, 3, 2) ; 0.00866869046330557
Precision: 4.58e-06
match ;  Bader value 1      ; LINEFIELD(static/bader.y=0\,z=0, 6, 2) ; 0.00993837077043741
Precision: 4.47e-06
match ;  Bader value 2      ; LINEFIELD(static/bader.y=0\,z=0, 10, 2) ; 0.0115895603915994


